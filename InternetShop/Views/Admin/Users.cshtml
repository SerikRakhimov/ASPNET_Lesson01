<h3 class="text-success">Список пользователей</h3>

<table class="table table-hover">
    <thead>
        <tr>
            <th width="150">Id</th>
            <th>Логин</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in ViewBag.Users)
        {

            <tr>
                <td><h4 class="text-primary">@user.Id</h4></td>
                <td>
                    <h4 class="text-primary"><a href="/Admin/User/@user.Id">@user.Login</a></h4>
                </td>
            </tr>
        }
    </tbody>
</table>
